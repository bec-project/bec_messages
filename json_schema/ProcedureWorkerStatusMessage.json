{
  "description": "Message type for sending procedure worker status updates\n\nArgs:\n    worker_queue (str): Worker queue ID\n    status (str): Worker status\n    current_execution_id (str | None): ID of the current job, only allowed for RUNNING",
  "properties": {
    "metadata": {
      "additionalProperties": true,
      "title": "Metadata",
      "type": "object"
    },
    "worker_queue": {
      "title": "Worker Queue",
      "type": "string"
    },
    "status": {
      "$ref": "ProcedureWorkerStatus.json"
    },
    "current_execution_id": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Current Execution Id"
    },
    "bec_codec": {
      "$ref": "BecCodecInfo.json",
      "readOnly": true
    }
  },
  "required": [
    "worker_queue",
    "status",
    "bec_codec"
  ],
  "title": "ProcedureWorkerStatusMessage",
  "type": "object"
}
